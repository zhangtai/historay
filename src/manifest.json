{
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "description": "Gives a popup to list histories related to current domain.",
    "homepage_url": "https://github.com/zhangtai/historay",
    "page_action": {
        "default_title": "Historay",
        "default_popup": "index.html",
        "default_icon": "icons/history.svg"
    },
    "icons": {
        "48": "icons/history.svg",
        "96": "icons/history.svg"
    },
    "permissions": [
        "activeTab",
        "history",
        "tabs"
    ],
    "manifest_version": 2,
    "name": "Historay",
    "version": "0.1",
    "browser_specific_settings": {
        "gecko": {
            "id": "tai.zhang@outlook.com"
        }
    }
}